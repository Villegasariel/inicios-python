class Alumno:
    def __init__(self, nombre, nota):
        self.nombre = nombre
        self.nota = nota

    def imprimir_datos(self):
        print(f"Nombre del alumno: {self.nombre}")
        print(f"Nota del alumno: {self.nota}")

    def resultado(self):
        if self.nota >= 7:
            print(f"El alumno {self.nombre} ha aprobado")
        else:
            print(f"El alumno {self.nombre} ha desaprobado")
            
# Creamos un objeto de la clase Alumno
alumno1 = Alumno("Juan", 8)
alumno2 = Alumno("Manuel", 5)
# Imprimimos los datos del alumno
alumno1.imprimir_datos()
alumno2.imprimir_datos()
# Mostramos el resultado del alumno
alumno1.resultado()
alumno2.resultado()         
